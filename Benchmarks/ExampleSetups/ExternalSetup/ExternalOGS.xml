<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <resources>
        <aboveground>
            <type> Default </type>
        </aboveground>
        <belowground>
            <type> OGSLargeScale3DExternal </type>
            <ogs_project_folder> Benchmarks/ExampleSetups/ExternalSetup </ogs_project_folder>
            <ogs_project_file> testmodel.prj </ogs_project_file>
            <source_mesh> source_domain.vtu </source_mesh>
            <bulk_mesh> testbulk.vtu </bulk_mesh>
            <abiotic_drivers>
                <seaward_salinity> 0.035 </seaward_salinity>
            </abiotic_drivers>
        </belowground>
    </resources>
    <plant_dynamics>
        <type> Default </type>
    </plant_dynamics>
    <population>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 10 </y_2>
                </domain>
                <n_individuals> 10 </n_individuals>
            </distribution>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 1e9 </t_end>
        <delta_t> 1e7 </delta_t>
    </time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <output>
        <type> OneTreeOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> Benchmarks/ExampleSetups/ExternalSetup/TreeOutput </output_dir>
        <allow_previous_output>True</allow_previous_output>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <growth_output> growth </growth_output>
    </output>
</MangaProject>

