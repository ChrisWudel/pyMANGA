<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleTest </type>
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 5 </x_2>
                <y_2> 5 </y_2>
	        <x_resolution> 20 </x_resolution>
	        <y_resolution> 20 </y_resolution>
            </domain>
        </aboveground_competition>
        <belowground_competition>
            <type> SimpleHydro </type>
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 6 </x_2>
                <y_2> 20 </y_2>
	        <x_resolution> 3 </x_resolution>
	        <y_resolution> 10 </y_resolution>
            </domain>
            <depth> 1 </depth>
            <porosity> 0.5 </porosity>
            <dilution_frac_upper> 0.08 </dilution_frac_upper>
            <dilution_frac_lower> 0.018 </dilution_frac_lower>
            <diffusion_frac> 0.2 </diffusion_frac>
            <sea_salinity> 35 </sea_salinity>
            <ini_sal> 35 </ini_sal>
            <up_sal> 10 </up_sal>
            <slope> 0.002 </slope>
            <k_f> 0.00025 </k_f>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleBettina </type>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Recruiting </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 6 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 0 </n_individuals>
                <n_recruitment_per_step> 0 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 5 </x_2>
                    <y_2> 100 </y_2>
                </domain>
                <n_individuals> 10 </n_individuals>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 3153600000 </t_end>
        <delta_t> 432000 </delta_t>
    </tree_time_loop>
    <visualization>
        <type> SimplePyplot </type>
    </visualization>
    <tree_output>
        <type> NONE </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> ./ProjectLib/ExampleSetups/testoutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <geometry_output> r_crown </geometry_output>
        <geometry_output> r_root </geometry_output>
    </tree_output>
</MangaProject>

