<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleTest </type>
        </aboveground_competition>
        <belowground_competition>
            <type> FixedSalinity </type>
            <salinity> 0.08 0.09 </salinity>
            <min_x>0</min_x>
            <max_x>20</max_x>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleBettina </type>
            <mortality>MinGirthGrowth Random </mortality>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Recruiting </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 0 </n_individuals>
                <n_recruitment_per_step> 5 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 5 </n_individuals>
                <n_recruitment_per_step> 5 </n_recruitment_per_step>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 315360000 </t_end>
        <delta_t> 31536000 </delta_t> <!--Time step length and time span arbitrarily chosen, target: short model runtime-->
    </tree_time_loop>
    <visualization>
        <type> SimplePyplot </type>
    </visualization>
    <tree_output>
        <type> NONE </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> test/testoutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <parameter_output> k_geom </parameter_output>
    </tree_output>
</MangaProject>
